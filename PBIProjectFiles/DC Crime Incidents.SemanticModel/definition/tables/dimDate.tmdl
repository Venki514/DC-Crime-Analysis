				    #"Changed Types" = Table.TransformColumnTypes(#"Added DateKey", {
				        {"Date", type date}, {"Year", Int64.Type}, {"Quarter", type text}, {"Month", Int64.Type}, 
				        {"Month Name", type text}, {"Day", Int64.Type}, {"Day of Week", Int64.Type}, {"Day Name", type text}, 
