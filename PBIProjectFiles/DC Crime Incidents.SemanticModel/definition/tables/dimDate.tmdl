		source = ```
				let
				    // Get min/max dates from existing crime data
				    StartDate = DateTime.Date(List.Min(CrimeIncidents[START_DATE])),
				    EndDate = DateTime.Date(List.Max(CrimeIncidents[START_DATE])),
				
				    // Create continuous date sequence
				    DateList = List.Dates(StartDate, Duration.Days(EndDate - StartDate) + 1, #duration(1,0,0,0)),
				    
				    // Convert to table
				    DateTable = Table.FromList(DateList, Splitter.SplitByNothing(), {"Date"}),
				
				    // Add date components
				    #"Added Year" = Table.AddColumn(DateTable, "Year", each Date.Year([Date]), Int64.Type),
				    #"Added Quarter" = Table.AddColumn(#"Added Year", "Quarter", each "Q" & Text.From(Date.QuarterOfYear([Date])), type text),
				    #"Added Month" = Table.AddColumn(#"Added Quarter", "Month", each Date.Month([Date]), Int64.Type),
				    #"Added Month Name" = Table.AddColumn(#"Added Month", "Month Name", each Date.ToText([Date], "MMMM"), type text),
				    #"Added Day" = Table.AddColumn(#"Added Month Name", "Day", each Date.Day([Date]), Int64.Type),
				    #"Added Day of Week" = Table.AddColumn(#"Added Day", "Day of Week", each Date.DayOfWeek([Date], Day.Monday) + 1, Int64.Type),
				    #"Added Day Name" = Table.AddColumn(#"Added Day of Week", "Day Name", each Date.ToText([Date], "dddd"), type text),
				    #"Added Week of Year" = Table.AddColumn(#"Added Day Name", "Week of Year", each Date.WeekOfYear([Date]), Int64.Type),
				    #"Added ISO Week Number" = Table.AddColumn(#"Added Week of Year", "ISO Week Number", each Date.WeekOfYear([Date], Day.Monday), Int64.Type),
				    #"Added Year Quarter" = Table.AddColumn(#"Added ISO Week Number", "Year-Quarter", each Text.From(Date.Year([Date])) & "-Q" & Text.From(Date.QuarterOfYear([Date])), type text),
				    #"Added Year Month" = Table.AddColumn(#"Added Year Quarter", "Year-Month", each Text.From(Date.Year([Date])) & "-" & Text.PadStart(Text.From(Date.Month([Date])),2, "0"), type text),
				    #"Added Fiscal Year" = Table.AddColumn(#"Added Year Month", "Fiscal Year", each if Date.Month([Date]) >= 7 then Date.Year([Date]) + 1 else Date.Year([Date]), Int64.Type),
				    #"Added Fiscal Quarter" = Table.AddColumn(#"Added Fiscal Year", "Fiscal Quarter", each "Q" & Text.From(if Date.QuarterOfYear([Date]) >= 3 then Date.QuarterOfYear([Date]) - 2 else Date.QuarterOfYear([Date]) + 2), type text),
